<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="f_devs_geo.form_type_point.class">FDevs\GeoBundle\Form\Type\PointType</parameter>
    </parameters>

    <services>
        <service id="f_devs_geo.form_type_point" class="%f_devs_geo.form_type_point.class%">
            <tag name="form.type" alias="fdevs_geo_point"/>
            <call method="setDefaultCoordinates"><argument>%f_devs_geo.default_coordinates%</argument></call>
        </service>
    </services>
</container>
